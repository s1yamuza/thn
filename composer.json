{
    "name": "s1yamuza/thn",
    "description": "THN Test Backend Default",
    "type": "project",
    "license": "MIT License",
    "authors": [
        {
            "name": "Sergio Yamuza",
            "email": "sergioyamuza@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/phpunit-bridge": "^5.2"
    },
    "config": {
    },
    "autoload": {
        "psr-4": {
            "THN\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "THN\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
